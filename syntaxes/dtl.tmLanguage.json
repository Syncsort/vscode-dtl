{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Syncsort DTL",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#environment_variables"
		},
		{
			"include": "#jobdtl_options"
		},
		{
			"include": "#taskdtl_options"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#strings_double_quote"
		},
		{
			"include": "#strings_single_quote"
		}
	],
	"repository": {
		"jobdtl_options": {
			"patterns": [{
				"name": "support.class.dtl",
				"match": "(?i)/\\b(DTL|TASK|FLOW|MAPREDUCE|CUSTOMTASK|DEFAULTFLOW|SUBJOB|END)\\b"
			}]
		},
		"taskdtl_options": {
			"patterns": [{
				"name": "support.class.dtl",
				"match": "(?i)/\\b(COLLATINGSEQUENCE|CONDITION|DATADICTIONARY|DATASIZE|DBCOLUMNSEXTRACT|DBCONNECTION|DBDATA|DBINPUT|DBOUTPUT|DELIMITEDRECORDLAYOUT|DTL|END|FILE|FILTER|GROUPBY|INBUFFER|INFILE|INMAXRECORDLENGTH|INPIPE|INPROCEDURE|JOINKEYS|KEYS|LOGOPTIONS|MEMORY|MQCONNECTION|MQFIELDSEXTRACT|MQINPUT|MQOUTPUT|NOPROMPT|NULLIF|OUTBUFFER|OUTFILE|OUTPIPE|OUTPROCEDURE|PADBYTE|PARTITION|RECORDLAYOUT|REFORMAT|SERVERCONNECTION|SFDCCONNECTION|SFDCDATA|SFDCFIELDSEXTRACT|SFDCINPUT|SFDCOUTPUT|STABLE|SUMMARY|TASKDOCUMENTATION|TASKTYPE|VALUE|WORKSPACE|XMLEXTRACT)\\b"
				}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.dtl",
				"match": " (?i)\\b(ABORTIF|ACUCOBOL|ADDROUT|AGGREGATE|ALIAS|ALIGNED2|ALIGNED4|ALL|ALLOWREORDERING|ALLVALUELABELS|ALWAYS|AN|ANSI|ANY|APACHEAVRO|APACHEPARQUET|APPEND|ARGUMENTS|ASCENDING|ASCII|AUTHENTICATION|AUTHOR|AUTODETECT|AVERAGE|BASELAYOUT|BIGENDIAN|BITS|BL|BLO|BLOC|BLOCK|BLOCKED|BLOCKSIZE|BOM|BUCKET|BYCONDITION|BYDAY|BYFIELD|BYHOUR|BYMINUTE|BYMONTH|BYSECOND|BYSIZE|BYTES|BYYEAR|BZIP2|CALLONCE|CALLREPEATEDLY|CHARACTER|CLIENT|COBOL|COLLATINGSEQUENCES|COLUMNS|COLUMNSPACING|COMMENTS|COMMITINTERVAL|COMPANY|COMPRESSED|CONDITION|CONDITIONS|CONNECTDIRECT|CONTIGUOUS|COPY|COUNT|CP273GERMANY|CP277DENMARK|CP278SWEDEN|CP280ITALY|CP284SPAIN|CP285UK|CP297FRANCE|CP37USA|CP500GLOBAL|CP871ICELAND|CR|CREATE|CREATECOMPOSITE|CRLF|CUBE|CUSTOMSTREAM|DATABASECONNECTIONS|DATE|DATETIME|DAY|DB2|DBMS|DD|DEFAULT|DEFAULTFIXEDFORMAT|DELETE|DELIMITED|DERIVEDFIELDS|DESCENDING|DFLOAT|DIMENSIONSET|DIMENSIONSETS|DIRECT|DISCARDAFTER|DISCARDBLANK|DISCARDFIRST|DISCARDSHORT|DISKBLOCKS|DISPLAYRECORD|DMEXPRESS|DOCUMENTATION|DROPNULLINFORMATION|DTD|DY|DYNAMIC|EACHPARTITION|EBCDIC|ELSE|EMPTY|EN|ENCLOSEDBY|ENCODING|ENCRYPTEDPASSWORD|ENDOFTASK|EQ|ERRLIMIT|ERROR|EXECUTABLE|EXITSTATUS|EXPANDWILDCARDS|EXPORT|EXTERNAL|EXTRACTAS|FASCII|FASTLOAD|FDISCARDAFTER|FDISCARDBLANK|FDISCARDFIRST|FDISCARDSHORT|FEBCDIC|FIELD|FIELDS|FIELDSEPARATOR|FILE|FILLER|FIRST|FIXED|FLOAT|FLOW|FOLDED|FOLLOWINGTASK|FORTRANUNFORMATTED|FSKIPBLANK|FSKIPRECORD|FSTOPAFTER|FTP|FULLPATH|FUNCTIONCALL|GENERATEJOBSCRIPT|GIGABYTES|GZIP|HDFS|HEADER|HFTP|HIERARCHIES|HIERARCHY|HIGH|HIGHCOMPRESSION|HOUR|HTTP|HTTPS|IEEE|IF|INCLUDE|INCLUDEAFTER|INCLUDEBEFORE|INDEXED|INSERT|INSTANCES|INTEGER|JOIN|KERBEROSAD|KEYEDINDEX|KILOBYTES|LAST|LAYOUT|LAYOUTNAME|LAYOUTS|LEAF|LEFTSIDE|LEVEL|LF|LINES|LOB|LOCALE|LOW|LP|LS|LZ|MAINFRAME|MAINFRAMEFIXED|MAINFRAMEJOBCARD|MAINFRAMEUNBLOCKEDVARIABLE|MAINFRAMEVARIABLE|MAINFRAMEVSAM|MAPPINGS|MAPTO|MATCH|MAXIMUM|MEGABYTES|MERGE|METADATAID|MFCOBOL|MFFIXED|MFLINESEQUENTIAL|MFVARIABLE|MINIMUM|MINUTE|MIXEDENCODINGS|MM|MN|MODIFICATION|MON|MONTH|MOVEAFTER|MOVEBEFORE|MQCONNECTIONS|MULTINATIONAL|NAME|NAMEDPIPE|NAMESRECORD|NC|NE|NEWLINE|NEWPAGE|NL|NM|NOBLANK|NOCOLUMNHEADINGS|NODETAIL|NODUPLICATE|NOEMPTY|NOFIELDENCODINGS|NOFIELDSEPARATOR|NONDICTIONARY|NONE|NONPRINTABLE|NORMALIZE|NOSKIP|NOTDIRECT|NOTENCLOSED|NOTNULLABLE|NOWARN|NULL|NULLABLE|NULLBYTEPREFIX|NULLCHAR|NULLIFEMPTY|NUMBER|OBJECT|ODBC|OFF|OFFSPRING|OMIT|ON|ONBREAK|ONLY|OPEN|OPENBRACKET|ORACLE|ORPHANVALUES|OS|OUTPUT|OUTRECORDCOUNT|OVERWRITE|PAGENUMBER|PAGESTART|PAGETOP|PAIRED|PARAMETEREXTRACT|PARAMETERFILE|PARAMETERIN|PARAMETERS|PARENT|PARTITION|PARTITIONCOUNT|PARTITIONSIZE|PD|PNODE|PORT|POSITIONAL|PREAGGREGATED|PRESERVENULLINFORMATION|PRESORTED|PREVIOUSTASK|PRIMARYKEY|PUBLICKEY|QUERY|QUEUE|RANDOM|RDW4|RECORDNUMBER|RECORDS|RECURSIVE|RELATIVE|REPEAT|REPLACEWITH|RETAINARRAY|RETAINCOMPOSITE|RETAINIF|RFA|RFM|RIDX|RIGHTSIDE|ROLLUP|ROOT|ROUNDROBIN|ROUNDSIZE|RUNCOUNT|RUNTIMEVARIABLES|RUNTOTAL|SAPCONNECTIONS|SCHEMA|SECOND|SEQUENTIAL|SERVERCONNECTION|SERVERCONNECTIONS|SESSIONS|SFDCCONNECTIONS|SFTP|SHOWOPTIONS|SIDE1|SIDE2|SIZE|SKIPFIELDS|SKIPLEVELS|SLEEP|SN|SOR|SORT|SORTED|SORTEDOUTPUT|SOURCE|SOURCEANDTARGET|SOURCECOMPRESSIONFORMAT|SOURCEENCODING|SOURCES|SQL|START|STATISTICS|STATUS|STCL|STCR|STDIN|STDOUT|STLF|STR|STREAM|STRING|SUBCUBE|SUCCESSIF|SYBASE|SYMBOLS|SYNCSORT|TAB|TABL|TABLE|TABLEA|TABLEAU|TARGETS|TASKDOCUMENTATION|TASKID|TASKSETTING|TBLOCK|TENACITY|TERABYTES|TERADATA|TEXT|THEN|TIME|TIMEMASK|TIMESTAMP|TIMEZONE|TIMEZONEHOUR|TIMEZONEMINUTE|TITLE|TOTAL|TP|TREATAS|TRUNCATEANDINSERT|TS|UINTEGER|UNALIGNED|UNCOMPRESSED|UNIX|UNIXSORTDEFAULT|UNPAIRED|UNSELECTEDRECORDS|UPDATE|UPPER|USER|UTF-16|UTF-16BE|UTF-16LE|UTF-32|UTF-32BE|UTF-32LE|UTF-8|UTILITY|VAL|VALU|VALUE|VALUES|VARIABLE|VERIFY|VERIFYORDER|VERSION|VFC|VIEWAS|VSAM|VSCOBOL|WARN|WARNINGS|WEBHDFS|WHENMATCHED|WHENNOTMATCHED|WIDTH|WINDOWS|WORKDATABASE|WORKDIR|XML|YEAR|YY|ZD)\\b"
				}]
		},
		"operators": {
			"patterns": [{
				"name": "keyword.operator.dtl",
				"match": "(?i)\\b(AND|CT|GE|GT|LE|LT|MT|NOT|OR)\\b"
				}]
		},
		"functions": {
			"patterns": [{
				"name": "support.function.dtl",
				"match": "\\b(Abs|CRC32|CharacterLengthOf|DateAdd|DateDiff|DateLastDay|DatePart|Encode|EvaluateExpressionList|Extract|FindContainedValue|GetExternalFunctionValue|GetUserDefinedValue|IfCompares|IfContainsAny|IfEqualsAny|IfMatches|IfNull|IfRecordJoined|IfRecordOrigin|IfThenElse|InString|IsSubcubeRecord|IsValidDate|IsValidNumber|LengthOf|Lookup|MD5|Mod|Pad|Pow|Rand|RegExReplace|Replace|Round|SetUserDefinedValue|SourceFullName|SourceName|Sqrt|Substring|TargetRecordNumber|ToChar|ToDate|ToFile|ToLower|ToNumber|ToText|ToUpper|Today|Translate|Trim|Truncate|URLDecode)\\b"
			}]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.dtl",
				"begin": "#",
				"end": "\n"
			}]
		},
		"environment_variables": {
			"patterns": [{
				"name": "support.variable.dtl",
				"begin": "\\$",
				"end": "( |\n)"
			}]
		},
		"strings_double_quote": {
			"name": "string.quoted.double.dtl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.dtl",
					"match": "\\\\."
				}
			]
		},
		"strings_single_quote": {
			"name": "string.quoted.single.dtl",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.dtl",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.dtl"
}